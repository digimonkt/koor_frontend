{
  "appId": "org.reactjs.example.koor",
  "appName": "koor",
  "bundledWebRuntime": false,
  "npmClient": "yarn",
  "cleartext": true,
  "hostname": "koorjobs.com",
  "allowNavigation": [
    "*"
  ],
  "webDir": "build",
  "plugins": {
    "SplashScreen": {
      "launchAutoHide": false,
      "androidScaleType": "CENTER_CROP"
    },
    "FirebaseAuthentication": {
      "skipNativeAuth": false,
      "providers": [
        "apple.com",
        "facebook.com",
        "google.com"
      ]
    }
  },
  "android": {
    "namespace": "org.reactjs.example.koor"
  }
}
